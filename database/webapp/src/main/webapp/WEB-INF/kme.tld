<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>

    <tlib-version>1.0</tlib-version>
    <jsp-version>2.0</jsp-version>
    <short-name>kme</short-name>
    <uri>http://kuali.org/mobility</uri>
    <description>Kuali Mobility Enterprise tag library</description>

    <tag>
        <name>page</name>
        <tag-class>org.kuali.mobility.tags.PageTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for a page</description>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>title</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>content</name>
        <tag-class>org.kuali.mobility.tags.ContentTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for content</description>
    </tag>

    <tag>
        <name>definitionListView</name>
        <tag-class>org.kuali.mobility.tags.DefinitionListViewTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for definitionListView</description>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>filter</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
    </tag>
    
    <tag>
        <name>definitionListTerm</name>
        <tag-class>org.kuali.mobility.tags.DefinitionListTermTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for definitionListTerm</description>
    </tag>
    
    <tag>
        <name>definitionListDefinition</name>
        <tag-class>org.kuali.mobility.tags.DefinitionListDefinitionTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for definitionListDefinition</description>
    </tag>
    
    <tag>
        <name>unorderedList</name>
        <tag-class>org.kuali.mobility.tags.UnorderedListTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for unorderedList</description>
    </tag>
    
    <tag>
        <name>orderedList</name>
        <tag-class>org.kuali.mobility.tags.OrderedListTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for orderedList</description>
    </tag>
    
    <tag>
        <name>listItem</name>
        <tag-class>org.kuali.mobility.tags.ListItemTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for listItem</description>
    </tag>
    
    <tag>
        <name>section</name>
        <tag-class>org.kuali.mobility.tags.SectionTag</tag-class>
        <body-content>scriptless</body-content>
        <description>This tag is for section</description>
    </tag>
    
</taglib>
